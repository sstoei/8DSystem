@{
    ViewData["Title"] = "UnAuthorized";
    Layout = "~/Views/Shared/_Layout.cshtml";

}
<div class="row h-100">
    <div class="col-sm-10 col-md-8 col-lg-6 mx-auto d-table h-100">
        <div class="d-table-cell align-middle">
            <div class="text-center">
                <h1 class="display-1 font-weight-bold">401</h1>
                <p class="h1">Unauthorized</p>
                <p class="h2 font-weight-normal mt-3 mb-4">You are not authorized for this page</p>
                <a asp-controller="Auth" asp-action="Login" class="btn btn-primary btn-lg">Return to homepage</a>
            </div>
        </div>
    </div>
</div>
